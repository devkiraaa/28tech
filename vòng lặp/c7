// dem va in uoc theo thu tu be den lon
#include <stdio.h>
#include <math.h>

int main(){
      int n;
      scanf("%d", &n);
      int cnt = 0;
      for(int i = 1; i <= n; i++){
            if(n % i == 0){
                 ++cnt;
            }
      }
      printf("%d\n", cnt);
      for(int i = 1; i <= n; i++){
            if(n % i == 0)
                  printf("%d ", i);
      }
}
